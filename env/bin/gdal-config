#!/bin/sh
CONFIG_LIBS="-L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lgdal"
CONFIG_DEP_LIBS="-lcrypto -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -ltiledb  -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lpoppler -ljson-c -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lfreexl -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lgeos_c -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lwebp -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lkmlbase -lkmldom -lkmlengine -lkmlxsd -lkmlregionator -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lexpat -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lxerces-c -lpthread -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lopenjp2 -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lnetcdf -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lhdf5  -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lmfhdf -ldf -lgif -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lgeotiff -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lpng -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lcfitsio -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lpq -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lzstd -llzma -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lsqlite3 -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lproj  -L/Users/jsheppar/Dropbox (University of Oregon)/GitHub/snow_depth/env/lib -lsqlite3